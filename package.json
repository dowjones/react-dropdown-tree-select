{
  "name": "react-dropdown-tree-select",
  "version": "0.0.0-semantic-release",
  "description": "A React Dropdown with Checkboxes that supports Tree data",
  "main": "dist/react-dropdown-tree-select.js",
  "repository": "https://github.com/dowjones/react-dropdown-tree-select.git",
  "author": "pandah <hrusikesh.panda@dowjones.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/**/* && webpack --config webpack.config.js --bail",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "demo": "webpack --progress --colors --watch --config docs/webpack.config.js",
    "demo:bs": "webpack --progress --colors --watch --config docs/examples/bootstrap/webpack.config.js",
    "demo:mdl": "webpack --progress --colors --watch --config docs/examples/material/webpack.config.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix src docs webpack.config.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "cross-env NODE_ENV=test ava",
    "test:cov": "rimraf .nyc_output && nyc npm test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react-simple-dropdown": "^3.2.0"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "nyc": "^11.2.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "semantic-release": "^7.0.2",
    "sinon": "^4.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.5.6"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "ignore-styles",
      "jsdom-global/register",
      "./setupEnzyme"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/node_modules/**",
      "dist",
      "demo"
    ]
  }
}
